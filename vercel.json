{
  "functions": {
    "app/api/kanji-svg/[hex]/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/kanji-svg/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=86400"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "michikanji.com"
        }
      ],
      "destination": "https://www.michikanji.com/$1",
      "permanent": true
    }
  ]
}