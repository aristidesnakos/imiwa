{
  "functions": {
    "app/api/kanji-svg/[hex]/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/kanji-svg/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=86400"
        }
      ]
    }
  ]
}